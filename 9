public class Solution {
    public boolean isPalindrome(int x) {
       for (int i = 0; i < (String.valueOf(x).split("").length) / 2; i++) {
			if (!String.valueOf(x).split("")[i].equals(String.valueOf(x).split("")[String.valueOf(x).split("").length - 1 - i])) {
				return false;
			}
		}
		return true;
    }
}
